<% content_for :extended_header do %>
  <div class="c-extended-header">
    <div class="wrapper">
      <h1>Map versions</h1>
      <ul class="action-buttons">
        <li><%= link_to 'New Map Version', new_admin_map_version_path, class: 'c-button' %></li>
      </ul>
    </div>
  </div>
<% end %>

<%= include_gon %>

<ul class="c-tabs">
  <li class="tab"><a href="/admin/sites/">Sites</a></li>
  <li class="tab"><a href="/admin/users/">Users</a></li>
  <li class="tab"aria-selected="true"><a href="/admin/map_versions/">Maps</a></li>
</ul>

<%= javascript_pack_tag 'application' %>
<%= react_component('AdminTableView', {
  meta: {
    columns: ['version', 'position', 'description'],
    actions: ['edit', 'delete']
  },
  data: @formattedMaps
} ) %>
