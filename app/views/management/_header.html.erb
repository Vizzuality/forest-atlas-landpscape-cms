<div class="c-header">
  <div class="wrapper">
    <div>
      <div class="site-switcher js-site-switcher"></div>
      <% unless @breadcrumbs.blank? %>
        <ul class="breadcrumbs">
          <% @breadcrumbs.each do |item| %>
            <li><%= item %></li>
          <% end %>
        </ul>
      <% end %>
    </div>
    <div>
      <%= link_to 'Logout', auth_logout_url, {:method => :post} %>
    </div>
  </div>
</div>

