<%= render partial: 'shared/navigation_header', locals: {form_steps: @page.form_steps, id: @page.id, current_step: step} %>
<%= render partial: 'shared/errors', locals: { resource: @page } %>
<%= Gon::Base.render_data(camel_case: true, camel_depth: 3) %>


<%= form_for @page, url: wizard_path, method: :put do |f| %>

  <% content_for :body_class do %>
    class="-wide"
  <% end %>

  <div class="l-analysis-dashboard">
    <div class="main-container">
      <h2><%= @page.name %></h2>
      <p class="description">
        <%= @page.description %>
      </p>
      <div class="filters js-filters">
        Filters coming soon
      </div>
      <div class="map js-map">
      </div>
    </div>
    <div class="secondary-container">
      <div class="graph js-chart-1"></div>
      <div class="graph js-chart-2"></div>
      <div class="bookmarks">
        Bookmarks
      </div>
    </div>
  </div>
  <%= render partial: 'footer', locals: {f: f} %>
<% end %>
<%= render partial: 'shared/errors', locals: { resource: @page } %>
<%= console if Rails.env.development? %>
